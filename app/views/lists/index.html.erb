<%= tag.h1 "Wishlists" %>

<%= tag.ul class:"wishlists" do %>
  <% @lists.each do |list|%>
    <%= tag.li do %>
      <%= link_to (image_tag list.user.image), list_path(list) %>
    <% end %>
  <% end %>
<% end %>
<%= tag.br %>
<%= tag.br %>
<%= tag.br %>

<%= tag.h1 "Or Browse Gifts" %>
<%= render partial: 'gifts/gift_list', locals: {gifts: @gifts} %>
